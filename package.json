  },
  "homepage": "https://github.com/leandrocunha/findhotel#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-select": "^2.0.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
